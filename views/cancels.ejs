<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

 <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/css/all.min.css" integrity="sha512-1PKOgIY59xJ8Co8+NE6FZ+LOAZKjy+KY8iq0G4B3CyeY6wYHN3yt9PW0XpSriVlkMXe40PTKnXrLnZ9+fkDaog==" crossorigin="anonymous" />

<br>
<div class="container">
<% if(error && error.length>0){%>
<div class="alert alert-danger">
<%=error %>
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>
<% if(success && success.length>0){%>
<div class="alert alert-success">
<%=success %>
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>
</div>
<br>
<title>Status!</title>

<%if(key=="Canceled"){%>
<table class="table">
     <%orders.forEach(function(data){%>

  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Image</th>
     
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Road</th>
      <th scope="col">Locality</th>
      <th scope="col">City</th>
      <th scope="col">Landmark</th>


      <th scope="col">Product</th>
      <th scope="col">Price</th>
      
      <th scope="col">Quantity</th>
      <%if(data.size){%>
       
      <th scope="col">Size</th>


      <%}%>
     <%if(data.pay=="Paid"){%>

       <th scope="col">Ifsc</th>
       <th scope="col">Account</th>
       <th scope="col">Phone</th>
     <%}%>
       <th scope="col">Date</th>
       <th scope="col">Status</th>



    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row"><i class="fas fa-hand-point-right"></i></th>
      <td>
       
         
         <%if(data.urls=="URL"){%>
            
              <a><img src="<%=data.image%>" style="width:50px"></a>


          <%}else{%>
             
              <a><img src="/<%=data.image%>" style="width:50px"></a>


          <%}%>

       
      </td>
      <td><%=data.name%></td>
      <td><%=data.author.username%></td>
      <td><%=data.roadNumber%></td>
      <td><%=data.locality%></td>
      <td><%=data.city%></td>
      <td><%=data.landmark%></td>
      
      <td><%=data.productD%></td>
      <td><%=data.Price%></td>
           <td><%=data.qty%></td>
 
      <%if(data.size){%>
                    
              <td><%=data.size%></td>
      <%}%>
      <%if(data.pay=="Paid"){%>
        <%if(data.ifsc==""){%> 
         <td>User Have'nt Uploaded Yet</td>
        <%}else{%>
         <td><%=data.ifsc%></td>

        <%}%>
          <%if(data.account==""){%> 
            <td>User Have'nt Uploaded Yet</td>
          <%}else{%>
            <td><%=data.account%></td>

          <%}%>      
          <td><%=data.phone%></td>
       <%}%>
      <td><%=data.datefour.toLocaleString("en-UN")%></td>
      <td>
	      <%=data.update%>
</td>

    </tr>
  
  </tbody>
  
  <%})%>  

</table>
<%}else if(key=="Delivered"){%>
<table class="table">
     <%orders.forEach(function(data){%>

  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Image</th>
     
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Road</th>
      <th scope="col">Locality</th>
      <th scope="col">City</th>
      <th scope="col">Landmark</th>


      <th scope="col">Product</th>
      <th scope="col">Price</th>
      <th scope="col">Quantity</th>

      <%if(data.size){%>
            <th scope="col">Size</th>

      <%}%>
      

       <th scope="col">Date</th>
       <th scope="col">Status</th>



    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row"><i class="fas fa-hand-point-right"></i></th>
      <td>

          
         
          <%if(data.urls=="URL"){%>
            
              <a><img src="<%=data.image%>" style="width:50px"></a>


          <%}else{%>
             
              <a><img src="/<%=data.image%>" style="width:50px"></a>


          <%}%>


      </td>

      <td><%=data.name%></td>
      <td><%=data.author.username%></td>
      <td><%=data.phone%></td>
      <td><%=data.roadNumber%></td>
      <td><%=data.locality%></td>
      <td><%=data.city%></td>
      <td><%=data.landmark%></td>
      
      <td><%=data.productD%></td>
      <td><%=data.Price%></td>
      <td><%=data.qty%></td>

      <%if(data.size){%>
       <td><%=data.size%></td>
      <%}%>
      <td><%=data.datefour.toLocaleString("en-UN")%></td>
      <td>
        <%=data.update%>
</td>

    </tr>
  
  </tbody>
  <%})%>  

</table>
<%}%>
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
